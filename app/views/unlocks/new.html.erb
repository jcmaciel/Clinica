<h2>Reenviar instruções de desbloqueio</h2>

<% form_for resource_name, resource, :url => unlock_path(resource_name) do |f| %>
  <%= f.error_messages %>

  <p><%= f.label :email %></p>
  <p><%= f.text_field :email %></p>

  <p><%= f.submit "Reenviar instruções de desbloqueio" %></p>
<% end %>

<%= render :partial => "shared/devise_links" %>
